{
  "name": "phaser3-ts-snowpack-eslint",
  "version": "1.0.0.220827432",
  "description": "A modern Phaser 3 template with TypeScript, Snowpack & ESLint",
  "main": "src/main.ts",
  "engines": {
    "node": "17.x"
  },
  "scripts": {
    "start": "snowpack dev",
    "prebuild": "tsc && npm run lint",
    "build": "snowpack build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-run": "snowpack build && node index.js"
  },
  "author": "paulasantamaria",
  "license": "MIT",
  "dependencies": {
    "ejs": "3.1.8",
    "express": "4.18.1",
    "snowpack": "3.8.8",
    "phaser": "3.55.2",
    "typescript": "4.6.3",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0"
  }
}
